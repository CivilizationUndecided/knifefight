@namespace Facepunch.UI
@inherits PanelComponent

@implements IKillListener

<root>
    <BuyMenu />
    <Crosshair />
    <WeaponInfoComponent />

    <PlayerInfo />

    <MarkerSystem />
    <RoundStateDisplay />

    <DamageIndicator />
</root>

@code {
    public void OnPlayerKilled( Component killer, Component victim, float damage, Vector3 position, Vector3 force )
